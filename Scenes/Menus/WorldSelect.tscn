[sub_resource type="GDScript" id="GDScript_32lsb"]
script/source = "extends Control

var target_position = Vector2.ZERO
@onready var target_world := $\"Basic/Basic World\"

func _ready():
	target_position = position
	for arrow in get_tree().get_nodes_in_group(\"Arrow\"):
		arrow.update_enabled()
		
func _process(delta):
	var distance = target_position - position
	position += delta * distance * 8.0
"

